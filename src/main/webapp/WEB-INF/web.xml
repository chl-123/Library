<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>displayServlet</servlet-name>
        <servlet-class>com.cya.controller.DisplayServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>displayServlet</servlet-name>
        <url-pattern>/books/admin/displayServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>booksServlet</servlet-name>
        <servlet-class>com.cya.controller.BooksServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>booksServlet</servlet-name>
        <url-pattern>/books/admin/booksServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>noticeServlet</servlet-name>
        <servlet-class>com.cya.controller.NoticeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>noticeServlet</servlet-name>
        <url-pattern>/books/admin/noticeServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>userServlet</servlet-name>
        <servlet-class>com.cya.controller.UserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>userServlet</servlet-name>
        <url-pattern>/books/admin/userServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>bookTypeServlet</servlet-name>
        <servlet-class>com.cya.controller.BookTypeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>bookTypeServlet</servlet-name>
        <url-pattern>/books/admin/bookTypeServlet</url-pattern>
    </servlet-mapping>


</web-app>